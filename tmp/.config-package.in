	config PACKAGE_alfred
		tristate "alfred... A.L.F.R.E.D. - Almighty Lightweight Fact Remote Exchange Daemon"
		default y if DEFAULT_alfred
		default m if ALL
		select PACKAGE_libpthread if USE_GLIBC
		depends on IPV6
